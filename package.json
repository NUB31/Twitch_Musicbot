{
  "name": "musicbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear": "rm -r build | rm -r dist | rm -r assets | rm settings.json | rm music_artist.txt | rm music_name.txt",
    "setup": "ts-node src/install.ts",
    "dev": "nodemon --watch src/**/*.ts -L src/server.ts",
    "build": "rm -r build | rm -r dist | tsc && pkg build/server.js --target node16-win --out-dir dist && pkg build/install.js  --target node16-win --out-dir dist && rm -r build",
    "start": "rm -r build | tsc && node build/server.js"
  },
