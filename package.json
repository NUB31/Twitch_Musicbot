    "@types/download": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.3",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "download": "^8.0.0",
    "fs-extra": "^10.1.0",
    "moment": "^2.29.4",
    "ntsuspend": "^1.0.2",
    "socket.io": "^4.5.3",
    "youtube-search": "^1.1.6",
    "ytdl-core": "^4.11.2"
  }
